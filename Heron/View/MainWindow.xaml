<local:Window x:Class="CatWalk.Heron.View.MainWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:local="clr-namespace:CatWalk.Heron.View"
		mc:Ignorable="d"
		Title="{Binding Path=Title}"
		Height="{Binding Path=Height}"
		Width="{Binding Path=Width}"
		Left="{Binding Path=Left}"
		Top="{Binding Path=Top}"
		WindowState="{Binding Path=WindowState}"
		RestoreWindowState="{Binding Path=RestoreWindowState}">
</local:Window>
